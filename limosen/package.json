{
  "name": "jaen-template",
  "description": "Template for Jaen projects",
  "version": "1.0.0",
  "private": true,
  "author": "Florian Kleber",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "NODE_OPTIONS=--max-old-space-size=8192 gatsby develop -H 0.0.0.0",
    "start": "yarn develop",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 gatsby build",
    "deploy": "NODE_OPTIONS=--max-old-space-size=8192 gatsby build && wrangler pages deploy ./public",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@reach/router": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "flexsearch": "^0.7.43",
    "framer-motion": "^10.16.1",
    "gatsby": "^5.11.0",
    "gatsby-jaen-lens": "1.2.0",
    "gatsby-jaen-mailpress": "^2.1.1",
    "gatsby-plugin-cloudflare-pages": "^1.0.3",
    "gatsby-plugin-i18n-l10n": "^5.14.1",
    "gatsby-plugin-jaen": "*",
    "gatsby-plugin-remove-console": "^0.0.2",
    "github-slugger": "^2.0.0",
    "gqty": "^3.2.2",
    "hast-util-sanitize": "^5.0.1",
    "jaen": "*",
    "jaen-fields-mdx": "*",
    "prismjs": "^1.29.0",
    "quantum-circuit": "^0.9.218",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.4",
    "react-highlight-words": "^0.20.0",
    "react-intl": "6.8.9",
    "react-simple-code-editor": "^0.13.1",
    "react-slick": "^0.31.0",
    "react-tsparticles": "^2.12.2",
    "slick-carousel": "^1.8.1",
    "snek-query": "^0.0.114",
    "tsparticles": "^2.12.0",
    "use-debounce": "^10.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/github-slugger": "^2.0.0",
    "@types/node": "^20.3.3",
    "@types/prismjs": "^1.26.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-highlight-words": "^0.16.4",
    "@types/uuid": "^9.0.8",
    "@types/zxcvbn": "^4.4.4",
    "patch-package": "^8.0.0",
    "typescript": "^5.1.6"
  }
